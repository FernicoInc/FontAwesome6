	<files>
		<file src="..\icon.png" target="" />

		<!-- .Net Standard 1.4 -->
		<file src="..\FontAwesome6.Core\bin\ReleasePro\netstandard1.4\FontAwesome6.*" target="lib\netstandard1.4\" />

		<!-- .Net Standard 2.1 -->
		<file src="..\FontAwesome6.Core\bin\ReleasePro\netstandard2.1\FontAwesome6.*" target="lib\netstandard2.1\" />

		<!-- .Net 5.0 -->
		<file src="..\FontAwesome6.Core\bin\ReleasePro\net5.0\FontAwesome6.*" target="lib\net5.0\" />

		<!-- .Net 6.0 -->
		<file src="..\FontAwesome6.Core\bin\ReleasePro\net6.0\FontAwesome6.*" target="lib\net6.0\" />

		<!-- WinUI -->
		<file src="..\FontAwesome6.Fonts.WinUI\bin\ReleasePro\net6.0-windows10.0.17763.0\FontAwesome6.*" target="lib\net6.0-windows10.0.17763.0" />
	</files>
